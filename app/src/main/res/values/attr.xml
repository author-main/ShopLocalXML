<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextExt">
        <attr name="roundRadius" format="dimension" />
        <attr name="drawableAction" format="reference" />
        <attr name="showClearIcon" format="boolean" />
        <attr name="lossFocusOutside" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CheckableImageView">
        <attr name="tintCheckedDrawable"   format="color" />
        <attr name="tintUncheckedDrawable" format="color" />
    </declare-styleable>
    <declare-styleable name="RatingView">
        <attr name="tintStar" format="color" />
        <attr name="tintBackgroundStar" format="color" />
        <attr name="count" format="float" />
        <attr name="intervalStar" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ImageIndicator">
        <attr name="countImages" format="integer" />
        <attr name="selectedIndex" format="integer" />
        <attr name="sizeSymbol" format="dimension" />
        <attr name="intervalSymbol" format="dimension" />
    </declare-styleable>
</resources>